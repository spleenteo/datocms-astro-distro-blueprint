---
import { readFragment, type FragmentOf } from '@lib/datocms/graphql';
import { {{ pascalCase name }}Fragment } from './fragment';

interface Props {
  block: FragmentOf<typeof {{ pascalCase name }}Fragment>;
}

const { block } = Astro.props;

const unmaskedRecord = readFragment({{ pascalCase name }}Fragment, block);
---
<pre><code>{{ pascalCase name }}: {JSON.stringify(block, null, 2)}</code></pre>
